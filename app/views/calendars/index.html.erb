<%= render 'layouts/navbar' %>

<div class="container">
  <%= month_calendar events: @reservations do |date, reservations| %>
    <%= date %>
    <% reservations.each do |reservation| %>
        <div>
          <%= reservation.name %>
        </div>
      <% end %>
  <% end %>
</div>

<%= render 'layouts/footer' %>
